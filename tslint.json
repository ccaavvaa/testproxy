{
  "extends": "tslint:latest",
  "rules": {
    "no-bitwise": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "prefer-conditional-expression": false,
    "prefer-const": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "always"
        }
      }
    ],
    "no-unused-expression":false,
    "no-var-requires": false,
    "interface-name": [
      false
    ]
  }
}